{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "format": "prettier --write *.js",
    "lint": "eslint --fix *.js",
    "fix": "yarn run format && yarn run lint"
  },
  "build": {
    "appId": "com.electron.myapp",
    "productName": "MyApp",
    "directories": {
      "output": "./dist"
    },
    "files": [
      "./*",
      "./package.json",
      "./package-lock.json"
    ],
    "win": {
      "icon": "assets/win/icon_win.ico",
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^15.1.1",
    "electron-builder": "^22.13.1",
    "eslint": "^8.4.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.0"
  },
  "dependencies": {
    "bootstrap-icons": "^1.7.2",
    "bootstrap5": "^1.1.9",
    "jquery": "^3.6.0",
    "poppers.js": "^1.16.1"
  }
}
